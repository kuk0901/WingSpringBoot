<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.edu.wing.minusCategory">

    <resultMap type="com.edu.wing.minusCategory.domain.MinusCategoryVo" id="minusCategoryResultMap">
        <id column="CATEGORY_NO" property="categoryNo" />
        <result column="CATEGORY_NAME" property="categoryName" />
    </resultMap>

    <select id="minusCategorySelectList" resultMap="minusCategoryResultMap">
        SELECT CATEGORY_NO, CATEGORY_NAME
        FROM MINUS_CATEGORY
        ORDER BY CATEGORY_NO
    </select>

</mapper>